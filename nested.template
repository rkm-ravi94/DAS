nestedView('Projection') {
    views {
      listView('{{LABEL}}') {
           jobs {
                names('Configure-Tomcat-Job', 'Configure-Nginx-Job', 'Configure-Mysql-Job', 'Configure-Zabbix-Job')
            }
            columns {
                status()
                weather()
                name()
                lastSuccess()
                lastFailure()
            }
        }
    }
}
